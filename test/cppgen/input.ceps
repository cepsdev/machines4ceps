Systemstate SENSOR_SMUL;  //  StützenMagnetUntenLinks
Systemstate SENSOR_SMOL;  //  StützenMagnetObenLinks
Systemstate SENSOR_SMUR;  //  StützenMagnetUntenRechts
Systemstate SENSOR_SMOR;  //  StützenMagnetObenRechts


