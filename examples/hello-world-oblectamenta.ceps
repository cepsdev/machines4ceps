/*
* Run with 'ceps hello-world-oblectamenta.ceps'
*/

kind Event;Event E;

OblectamentaDataLabel msg;

oblectamenta{global{
 data{
  msg;"Hello, World!\n";
 };};
};

sm{
 S;    
 states{Initial;};
 Actions{
  doPrintHello{
   oblectamenta{text{asm{
    dbg_printsz(msg);
   };};};
  };
 };
 t{Initial;Initial;E;doPrintHello;};
};

Simulation{
 Start{S;};
 E;
};