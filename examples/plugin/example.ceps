kind Event;
Event   evCM_SLAC_PARM_REQ,
        evCM_SLAC_PARM_CNF,
        evCM_START_ATTEN_CHAR_IND,
        evCM_MNBC_SOUND_IND,
        evCM_ATTEN_PROFILE_IND,
        evCM_ATTEN_CHAR_RSP,
        evCM_VALIDATE_REQ,
        evCM_VALIDATE_CNF,
        evCM_SLAC_MATCH_REQ,
        evCM_SLAC_MATCH_CNF,
        evCM_SET_KEY_REQ,
        evCM_SET_KEY_CNF,
        evCM_AMP_MAP_REQ,
        evCM_AMP_MAP_CNF;


val CM_SLAC_PARM_REQ = 1;
val CM_SLAC_PARM_CNF = 2;
val CM_START_ATTEN_CHAR_IND = 3;
val CM_MNBC_SOUND_IND = 4;
val CM_ATTEN_PROFILE_IND = 5;
val CM_ATTEN_CHAR_RSP = 6;
val CM_VALIDATE_REQ = 7;
val CM_VALIDATE_CNF = 8;
val CM_SLAC_MATCH_REQ = 9;
