                    run_id{mme_run_id;};                                        
                });
            }else if(CM_SET_KEY_REQ == mme_type)
            {
                  evCM_SET_KEY_REQ(mme{
                    application_type{mme_application_type;};
                    security_type{mme_security_type;};
                    run_id{mme_run_id;};                                        
                });
            }else if(CM_SET_KEY_CNF == mme_type)
