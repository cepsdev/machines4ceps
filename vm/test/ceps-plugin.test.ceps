run_oblectamenta_bytecode(
	vm{
		kind OblectamentaOpcode;
		OblectamentaOpcode addi32,ldi32;

		stack{
			
		};
		data{0;0;
			
		};
		text{
			ldi32(1); // push immediate value 1 onto stack
			ldi32(2); // push immediate value 2 onto stack
			addi32;   // push 1 + 2 onto stack
		};
	}
);


